{
  "type": "postgres",
  "host": "localhost",
  "port": 5432,
  "username": "your_username",
  "password": "your_password",
  "database": "your_database",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/modules/**/*.entity.ts"
  ],
  "migrations": [
    "src/migration/**/*.ts"
